function CalcularPrecio () 
{
    //variables
    let cantidadLamparas;
    let marcaIngresada;
    let precioBruto;
    let descuento;
    let interes_IIBB;

    //obtencion de datos
    cantidadLamparas = parseInt(document.getElementById('txtIdCantidad').value);
    marcaIngresada = document.getElementById('Marca').value;
    
    precioBruto = cantidadLamparas * 35;
    
    //condicional if 
    if (cantLamparas > 5)
    {
       descuento = 50;
    }
    else
    {
       if(cantLamparas == 5)
       {
            if(marcaIngresada == "ArgentinaLuz")
            {
                descuento = 40; 
            }
            else
            {
                descuento = 30;
            }
       } 
       else
       {
           if(cantLampara == 4)
           {
               if(marcaIngresada == "ArgentinaLuz" || marcaIngresada == "FelipeLamparas")
               {
                   descuento = 25;
               }
               else
               {
                   descuento = 20;
               }
           }
           else
           {
               if(cantLampara == 3)
               {
                   if(marcaIngresada == "ArgentinaLuz")
                   {
                       descuento = 15;
                   }
                   else if(marcaIngresada == "FelipeLamparas")
                   {
                       descuento = 10;
                   } 
                   else 
                   {
                       descuento = 5;
                   }
               }
           }
       }
       precioBruto = precioBruto - (precioBruto * descuento/100);
       document.getElementById('').value = precioBruto;
       if (precioBruto > 120)
       {
           interes_IIBB = precioBruto + (precio bruto * 10/100);
           alert(`Usted pago ${interes_IIBB} de IIBB.`);
       }
    }
